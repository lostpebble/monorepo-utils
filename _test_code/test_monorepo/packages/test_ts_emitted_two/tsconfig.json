{
  "compilerOptions": {
    "declaration": true,
    "rootDir": "./src",
    "outDir": "./src",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["./src"],
  "exclude": ["_"]
}
