{
  "compilerOptions": {
    "noEmit": true,
    "preserveWatchOutput": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["ESNext"],
    "moduleResolution": "Bundler",
    "module": "ES2022" /* Specify what module code is generated. */,
    "target": "ESNext",
    "noImplicitAny": false,
    "declaration": true,
    "declarationMap": true
  }
}
